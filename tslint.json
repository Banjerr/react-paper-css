{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": false,
    "interface-over-type-literal": false,
    "ordered-imports": [
      true,
      {
        "grouped-imports": true
      }
    ],
    "prefer-array-literal": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
    ],
  }
}